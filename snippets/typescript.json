{
	"Log to the console": {
		"prefix": "log",
		"body": [
			"console.log($1);",
			"$0"
		],
		"description": "Log to the console"
	},
	"import": {
		"prefix": [
			"imp",
			"import"
		],
		"body": [
			"import $2 from \"$\";"
		]
	},
	"useState": {
		"prefix": "useState",
		"body": [
			"const [$1, set$1] = useState($2);"
		]
	},
	"useEffect": {
		"prefix": "useEffect",
		"body": [
			"useEffect(() => {",
			"  $1",
			"}, [$2]);"
		]
	},
	"useMemo": {
		"prefix": "useMemo",
		"body": [
			"const $1 = useMemo(() => {",
			"  $2",
			"}, [$3]);"
		]
	},
	"ternary": {
		"prefix": [
			"?"
		],
		"body": [
			"$1 ? $2 : $3"
		]
	},
	"oneLineLambda": {
		"prefix": [
			"f"
		],
		"body": [
			"($1) => $0"
		]
	},
	"multiLineLambda": {
		"prefix": [
			"ff"
		],
		"body": [
			"($1) => {",
			"  $1",
			"}"
		]
	}
}
